<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Should we vibrate on an icon animation of the shelf. This should only be active if the
    vibrator is capable of subtle vibrations -->
    <bool name="config_vibrateOnIconAnimation">true</bool>

    <!-- Preferred max refresh rate at keyguard, if supported by the display. -->
    <integer name="config_keyguardMaxRefreshRate">-1</integer>

    <!-- StatusBar burn-in protection -->
    <bool name="config_statusBarBurnInProtection">true</bool>

    <!-- Microns/ums (1000 um = 1mm) per pixel for the given device. If unspecified, UI that
         relies on this value will not be sized correctly. -->
    <item format="float" name="pixel_pitch" type="dimen">55.18</item>
</resources>
